// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model FileMetadata {
  id           Int    @id @default(autoincrement())
  file_id      Int    @unique
  key          String
  package_name String
  size         String
  topic_id     Int
  file_url     String
  title        String
}
